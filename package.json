{
  "name": "react-align",
  "version": "2.0.6",
  "author": "KaWaite",
  "module": "dist/react-align.esm.js",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "type": "tsc",
    "test": "vitest",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "resolutions": {
    "**/@types/react": "18.0.15"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/react-align.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-align.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@size-limit/preset-small-lib": "^6.0.3",
    "@svgr/rollup": "^6.2.1",
    "@types/react": "^18.0.15",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^8.2.5",
    "cssnano": "^5.1.12",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^6.0.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-plugin-dts": "^1.3.0",
    "vitest": "^0.18.0"
  },
  "dependencies": {
    "glamor": "^2.20.40",
    "react-beautiful-dnd": "^13.1.0"
  }
}
